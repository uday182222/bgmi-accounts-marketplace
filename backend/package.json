{
  "name": "bgmi-accounts-backend",
  "version": "1.0.0",
  "description": "Backend API for BGMI Accounts Service",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts",
    "migrate:dev": "NODE_ENV=development npm run migrate",
    "migrate:prod": "NODE_ENV=production npm run migrate"
  },
  "keywords": [
    "bgmi",
    "gaming",
    "accounts",
    "marketplace",
    "api"
  ],
  "author": "BGMI Accounts Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.882.0",
    "@aws-sdk/client-dynamodb": "^3.882.0",
    "@aws-sdk/client-kms": "^3.883.0",
    "@aws-sdk/client-s3": "^3.882.0",
    "@aws-sdk/client-ses": "^3.883.0",
    "@aws-sdk/client-sns": "^3.883.0",
    "@aws-sdk/s3-request-presigner": "^3.883.0",
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "stripe": "^14.25.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
